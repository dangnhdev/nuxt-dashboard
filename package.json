{
  "name": "nuxt-ui-template-dashboard",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "bunx concurrently -k -p \"[{name}]\" -n \"NUXT,CONVEX\" -c \"green,magenta\" \"bun dev:nuxt\" \"bun dev:convex\"",
    "dev:nuxt": "nuxt dev --dotenv .env.local",
    "dev:convex": "convex dev",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.71",
    "@iconify-json/simple-icons": "^1.2.55",
    "@nuxt/ui": "^4.1.0",
    "@unovis/ts": "^1.6.1",
    "@unovis/vue": "^1.6.1",
    "@vueuse/nuxt": "^13.9.0",
    "convex": "^1.28.0",
    "convex-nuxt": "^0.1.5",
    "date-fns": "^4.1.0",
    "nuxt": "^4.2.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.9.0",
    "eslint": "^9.38.0",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.1.2"
  },
  "resolutions": {
    "unimport": "4.1.1"
  },
  "packageManager": "pnpm@10.19.0"
}
